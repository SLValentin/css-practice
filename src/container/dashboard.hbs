<html>

  <head>
    <title>
      Dashboard Page
    </title>
    <meta charset='utf-8' />
    <link rel='apple-touch-icon' sizes='180x180' href='/fav/apple-touch-icon.png' />
    <link rel='icon' type='image/png' sizes='32x32' href='/fav/favicon-32x32.png' />
    <link rel='icon' type='image/png' sizes='16x16' href='/fav/favicon-16x16.png' />
    <link rel='manifest' href='/fav/site.webmanifest' />
    <link rel='mask-icon' href='/fav/safari-pinned-tab.svg' color='#5bbad5' />
    <meta name='msapplication-TileColor' content='#da532c' />

    <meta name='theme-color' content='#ffffff' />

    <meta name='viewport' content=' viewport-fit=cover, with=device-with, initial-scale=1.0 minimum-scale=1.0, maximum-scale=1.0, user-scalable=no' />
    <link href='css/normalize.css' rel='stylesheet' />

    <link href='css/dashboard.css' rel='stylesheet' />

  </head>

  <body>

    <div class='page page-dashboard'>
      <aside class='page-aside aside vertical'>
        <div class='aside-logo text'>Sidebar</div>
        <nav class='vertical vertical-small'>
          <a href='#' class='nav-button nav-button-active click'>
            <span class='icon nav-button-icon' style='background-image: url(/svg/nav/home.svg);'> </span>
            <span class='text text-light'>Home</span>
          </a>
          <a href='#' class='nav-button click'>
            <span class='icon nav-button-icon' style='background-image: url(/svg/nav/table.svg);'> </span>
            <span class='text text-light'>Orders</span>
          </a>
          <a href='#' class='nav-button click'>
            <span class='icon nav-button-icon' style='background-image: url(/svg/nav/grid.svg);'> </span>
            <span class='text text-light'>Products</span>
          </a>
        </nav>
      </aside>

      <header class='header'>
        <div class='layout header-content text-small'>
          <div class='header-search'>
            <input type='text' placeholder='Search' class='field field-header' />
            <span class='icon header-icon-search' style='background-image: url(/svg/header/search.svg);'></span>
            <span class='icon icon-header-settings icon-button icon-color-blue click' style='background-image: url(/svg/header/settings.svg);'></span>
          </div>
          <div class='header-icon-list'>
            <span class='icon icon-header-antena icon-button click icon-color-light-yellow' style='background-image: url(/svg/header/antena.svg);'></span>
            <span class='icon icon-header-symbols icon-button click icon-color-light-blue' style='background-image: url(/svg/header/symbols.svg);'></span>
            <span class='icon icon-header-bell icon-button click icon-color-light-yellow' style='background-image: url(/svg/header/bell.svg);'></span>
          </div>

        </div>

      </header>
      <main class='main'>
        <div class='layout'>
          <section class='box analytic-block'>

            <div class='analytic analytic-double analytic-purpule'>
              <span class='icon analytic-button icon-button click icon-color-purpule' style='background-image: url(/svg/analytic/money.svg);'></span>

              <div class='analytic-content vertical vertical-small'>
                <div class='analytic-title text text-light'>$500.00</div>
                <div class='text text-lighter text-small'>Total earning</div>

                <span class='icon analytic-arrow' style='background-image: url(/svg/analytic/arrow.svg);'></span>
              </div>

            </div>

            <div class='analytic analytic-double analytic-blue'>
              <span class='icon analytic-button icon-button click icon-color-blue' style='background-image: url(/svg/analytic/order.svg);'></span>

              <div class='analytic-content vertical vertical-small'>
                <div class='analytic-title text text-light'>$961</div>
                <div class='text text-lighter text-small'>Total order</div>
              </div>

            </div>

            <div class='analytic analytic-row analytic-blue'>
              <span class='icon analytic-button icon-button click icon-color-blue' style='background-image: url(/svg/analytic/money.svg);'></span>

              <div class='analytic-content vertical vertical-small'>
                <div class='analytic-small-title text text-light'>$203k</div>
                <div class='text text-lighter text-small'>Total Income</div>
              </div>

            </div>

            <div class='analytic analytic-row analytic-light'>
              <span class='icon analytic-button icon-button click icon-color-light-yellow' style='background-image: url(/svg/analytic/iconshop.svg);'></span>

              <div class='analytic-content vertical vertical-small'>
                <div class='analytic-small-title text'>$203k</div>
                <div class='text text-small'>Total Income</div>
              </div>

            </div>
          </section>

          <section class='payment-block'>
            <div class='box purchase'>
              <span class='text text-heading'>Purchase list</span>
              <hr />
              <div class='purchase-list'>
                <div class='purchase-card vertical vertical-small'>
                  <span class='text text-small'>ID437661</span>
                  <span class='text text-small'>May 11, 2023, 14:21</span>
                </div>
                <div class='purchase-card vertical vertical-small'>
                  <span class='text text-small'>AirOn ProCam</span>
                  <span class='text text-small'>7/8 USB Type-C</span>
                </div>
                <div class='purchase-card vertical vertical-small'>
                  <span class='text text-small'>132.32 EUR</span>
                  <span class='text text-small text-warning'>Not paid</span>
                </div>
                <div class='purchase-card vertical vertical-small'>
                  <span class='text text-small'>Customer</span>
                  <span class='text text-small'>robin@com.de</span>
                </div>

              </div>
              <hr />
              <div class='purchase-list'>
                <div class='purchase-card vertical vertical-small'>
                  <span class='text text-small'>ID437661</span>
                  <span class='text text-small'>May 11, 2023, 14:21</span>
                </div>
                <div class='purchase-card vertical vertical-small'>
                  <span class='text text-small'>PHILIPS Essential</span>
                  <span class='text text-small'>HD9200/90</span>
                </div>
                <div class='purchase-card vertical vertical-small'>
                  <span class='text text-small'>543 USD</span>
                  <span class='text text-small text-success'>Pay by Stripe</span>
                </div>
                <div class='purchase-card vertical vertical-small'>
                  <span class='text text-small'>Customer</span>
                  <span class='text text-small'>dima@com.ua</span>
                </div>

              </div>

            </div>

            <div class='box payment'>
              <span class='text text-heading'>Payment</span>
              <hr />
              <div class='block-radio vertical vertical-small'>
                <div>
                  <input type='radio' checked />
                  <label class='text text-small'>Credit card</label>
                </div>
                <div>
                  <input type='radio' />
                  <label class='text text-small'>Debit card</label>
                </div>
                <div>
                  <input type='radio' />
                  <label class='text text-small'>PayPal</label>
                </div>

              </div>

              <div class='block-input'>
                <div class='payment-field vertical vertical-small'>
                  <span class='text text-small'>Name on card</span>
                  <input type='text' class='field' />
                </div>
                <div class='payment-field vertical vertical-small'>
                  <span class='text text-small'>Credit card number</span>
                  <input type='text' class='field' />
                </div>
                <div class='payment-field vertical vertical-small'>
                  <span class='text text-small'>Expiration</span>
                  <input type='text' class='field' />
                </div>
                <div class='payment-field vertical vertical-small'>
                  <span class='text text-small'>CVV</span>
                  <input type='text' class='field' />
                </div>

              </div>
              <hr />

              <div class='payment-select'>
                <div class='vertical vertical-small'>
                  <span class='text text-small'>Country</span>
                  <div class='field field-option'>
                    <span class='text'>Choose...</span>
                    <select class='field-select'>
                      <option value='1234'>Option</option>
                    </select>
                  </div>
                </div>
                <div class='select-country vertical vertical-small'>
                  <span class='text text-small'>State</span>
                  <div class='field field-option'>
                    <span class='text'>Choose...</span>
                    <select class='field-select'>
                      <option value='1234'>Option</option>
                    </select>
                  </div>
                </div>

              </div>
              <hr />

              <a href='#' class='payment-button nav-button-active click'>
                <span class='text text-light text-small'>Continue to checkout</span>
              </a>

            </div>

          </section>
        </div>

      </main>

    </div>

  </body>

</html>